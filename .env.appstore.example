# App Store Connect API Configuration
# Copy this file to .env.appstore and fill in your actual values
# NEVER commit .env.appstore to version control!

# Your Apple ID email (used for App Store Connect)
APPLE_ID=your-apple-id@example.com

# Your Team ID (10-character string)
# Find it at: https://developer.apple.com/account (Membership section)
TEAM_ID=YOUR_TEAM_ID

# iTunes Connect Team ID (usually same as TEAM_ID)
ITC_TEAM_ID=YOUR_TEAM_ID

# App Store Connect API Key Information
# Find these at: https://appstoreconnect.apple.com > Users and Access > Keys

# Key ID (already configured: 2A6UCBGW5Z)
APP_STORE_CONNECT_API_KEY_ID=2A6UCBGW5Z

# Issuer ID (UUID format: xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx)
APP_STORE_CONNECT_API_ISSUER_ID=YOUR_ISSUER_ID_HERE

# Path to your .p8 key file (relative to ios directory)
APP_STORE_CONNECT_API_KEY_PATH=../AuthKey_2A6UCBGW5Z.p8
